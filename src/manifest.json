{
    "manifest_version": 3,
    "name": "Twitter Blink Unfurler",
    "version": "1.0",
    "description": "Unfurls specific links on Twitter using Blinks",
    "permissions": ["activeTab"],
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["https://twitter.com/*", "https://x.com/*"],
        "js": ["content.js"]
      }
    ]
  }

